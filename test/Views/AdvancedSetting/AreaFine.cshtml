<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //传入初值
        $.ajax({
            url: '/AdvancedSetting/SendDataMJWT', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#mjwt1').val(val.mjwt1);
                $('#mjwt2').val(val.mjwt2);
                $('#mjwt3').val(val.mjwt3);
                $('#mjwt4').val(val.mjwt4);
                $('#mjwt5').val(val.mjwt5);
                $('#mjwt6').val(val.mjwt6);
                $('#mjwt7').val(val.mjwt7);
                $('#mjwt8').val(val.mjwt8);
                $('#mjwt9').val(val.mjwt9);
                $('#mjwt10').val(val.mjwt10);
                $('#mjwt11').val(val.mjwt11);
                $('#mjwt12').val(val.mjwt12);
                $('#mjwt13').val(val.mjwt13);
                $('#mjwt14').val(val.mjwt14);
                $('#mjwt15').val(val.mjwt15);
                $('#mjwt16').val(val.mjwt16);
                $('#mjwt17').val(val.mjwt17);
                $('#mjwt18').val(val.mjwt18);
                $('#mjwt19').val(val.mjwt19);
                $('#mjwt20').val(val.mjwt20);
            }
        });
    });
    function postDataMJWT() {
        var mjwt1 = $("#mjwt1").val();
        var mjwt2 = $("#mjwt2").val();
        var mjwt3 = $("#mjwt3").val();
        var mjwt4 = $("#mjwt4").val();
        var mjwt5 = $("#mjwt5").val();
        var mjwt6 = $("#mjwt6").val();
        var mjwt7 = $("#mjwt7").val();
        var mjwt8 = $("#mjwt8").val();
        var mjwt9 = $("#mjwt9").val();
        var mjwt10 = $("#mjwt10").val();
        var mjwt11 = $("#mjwt11").val();
        var mjwt12 = $("#mjwt12").val();
        var mjwt13 = $("#mjwt13").val();
        var mjwt14 = $("#mjwt14").val();
        var mjwt15 = $("#mjwt15").val();
        var mjwt16 = $("#mjwt16").val();
        var mjwt17 = $("#mjwt17").val();
        var mjwt18 = $("#mjwt18").val();
        var mjwt19 = $("#mjwt19").val();
        var mjwt20 = $("#mjwt20").val();

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/AdvancedSetting/postDataMJWT', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: {
                deviceID: va, fileName: va1,
                mjwt1: mjwt1, mjwt2: mjwt2, mjwt3: mjwt3, mjwt4: mjwt4, mjwt5: mjwt5,
                mjwt6: mjwt6, mjwt7: mjwt7, mjwt8: mjwt8, mjwt9: mjwt9, mjwt10: mjwt10,
                mjwt11: mjwt11, mjwt12: mjwt12, mjwt13: mjwt13, mjwt14: mjwt14, mjwt15: mjwt15,
                mjwt16: mjwt16, mjwt17: mjwt17, mjwt18: mjwt18, mjwt19: mjwt19, mjwt20: mjwt20,
            },        //要传递的数据
            success: function (result) {
                if (result == "Success") {
                    alert("提交成功！");
                }
                else {
                    alert("提交失败！");
                }
            }
        });
    }
</script>

