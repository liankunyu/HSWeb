<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //初始化
            $.ajax({
                url: '/AdvancedSetting/SendDataGZYSD', //后台处理程序
                type: 'post',         //数据发送方式
                dataType: 'text',     //接受数据格式
                data: { deviceID: va, fileName: va1 },        //要传递的数据
                success: function (result) {

                    var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                    $('#gzysd1').val(val.gzysd1);
                    $('#gzysd2').val(val.gzysd2);
                    $('#gzysd3').val(val.gzysd3);
                    $('#gzysd4').val(val.gzysd4);
                    $('#gzysd5').val(val.gzysd5);
                    $('#gzysd6').val(val.gzysd6);
                    $('#gzysd7').val(val.gzysd7);
                    $('#gzysd8').val(val.gzysd8);
                    $('#gzysd9').val(val.gzysd9);
                    $('#gzysd10').val(val.gzysd10);
                    $('#gzysd11').val(val.gzysd11);
                    $('#gzysd12').val(val.gzysd12);
                    $('#gzysd13').val(val.gzysd13);
                    $('#gzysd14').val(val.gzysd14);
                    $('#gzysd15').val(val.gzysd15);
                    $('#gzysd16').val(val.gzysd16);
                    $('#gzysd17').val(val.gzysd17);
                    $('#gzysd18').val(val.gzysd18);
                    $('#gzysd19').val(val.gzysd19);
                    $('#gzysd20').val(val.gzysd20);
                }
            });
        });
        function postDataGZYSD() {
            var gzysd1 = $("#gzysd1").val();
            var gzysd2 = $("#gzysd2").val();
            var gzysd3 = $("#gzysd3").val();
            var gzysd4 = $("#gzysd4").val();
            var gzysd5 = $("#gzysd5").val();
            var gzysd6 = $("#gzysd6").val();
            var gzysd7 = $("#gzysd7").val();
            var gzysd8 = $("#gzysd8").val();
            var gzysd9 = $("#gzysd9").val();
            var gzysd10 = $("#gzysd10").val();
            var gzysd11 = $("#gzysd11").val();
            var gzysd12 = $("#gzysd12").val();
            var gzysd13 = $("#gzysd13").val();
            var gzysd14 = $("#gzysd14").val();
            var gzysd15 = $("#gzysd15").val();
            var gzysd16 = $("#gzysd16").val();
            var gzysd17 = $("#gzysd17").val();
            var gzysd18 = $("#gzysd18").val();
            var gzysd19 = $("#gzysd19").val();
            var gzysd20 = $("#gzysd20").val();

            var va = getCookie("deviceID");
            var va1 = getCookie("fileName");
            $.ajax({
                url: '/AdvancedSetting/postDataGZYSD', //后台处理程序
                type: 'post',         //数据发送方式
                dataType: 'text',     //接受数据格式
                data: {
                    deviceID: va, fileName: va1,
                    gzysd1: gzysd1, gzysd2: gzysd2, gzysd3: gzysd3, gzysd4: gzysd4, gzysd5: gzysd5,
                    gzysd6: gzysd6, gzysd7: gzysd7, gzysd8: gzysd8, gzysd9: gzysd9, gzysd10: gzysd10,
                    gzysd11: gzysd11, gzysd12: gzysd12, gzysd13: gzysd13, gzysd14: gzysd14, gzysd15: gzysd15,
                    gzysd16: gzysd16, gzysd17: gzysd17, gzysd18: gzysd18, gzysd19: gzysd19, gzysd20: gzysd20,
                },        //要传递的数据
                success: function (result) {
                    if (result == "Success") {
                        alert("提交成功！");
                    }
                    else {
                        alert("提交失败！");
                    }
                }
            });
        }
</script>

