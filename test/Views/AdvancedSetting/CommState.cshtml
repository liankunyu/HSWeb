<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
 

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/AdvancedSetting/SendDataCommState', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#zhuban').val(val.zhuban);
                $('#xiangji1').val(val.xiangji1);
                $('#xiangji2').val(val.xiangji2);
                $('#xiangji3').val(val.xiangji3);
                $('#xiangji4').val(val.xiangji4);
                $('#xiangji5').val(val.xiangji5);
                $('#xiangji6').val(val.xiangji6);
                $('#xiangji7').val(val.xiangji7);
                $('#xiangji8').val(val.xiangji8);
                $('#xiangji9').val(val.xiangji9);
                $('#xiangji10').val(val.xiangji10);
                $('#xiangji11').val(val.xiangji11);
                $('#xiangji12').val(val.xiangji12);
                $('#xiangji13').val(val.xiangji13);
                $('#xiangji14').val(val.xiangji14);
                $('#xiangji15').val(val.xiangji15);
                $('#xiangji16').val(val.xiangji16);
                $('#xiangji17').val(val.xiangji17);
                $('#xiangji18').val(val.xiangji18);
                $('#xiangji19').val(val.xiangji19);
                $('#xiangji20').val(val.xiangji20);
            }
        });
    });
  
</script>

