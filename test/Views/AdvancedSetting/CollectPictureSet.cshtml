<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //传入初值
        $.ajax({
            url: '/AdvancedSetting/SendDataCTSZ', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#ctsz1').val(val.ctsz1);
                $('#ctsz2').val(val.ctsz2);
                $('#ctsz3').val(val.ctsz3);
                $('#ctsz4').val(val.ctsz4);
                $('#ctsz5').val(val.ctsz5);
                $('#ctsz6').val(val.ctsz6);
                $('#ctsz7').val(val.ctsz7);
                $('#ctsz8').val(val.ctsz8);
                $('#ctsz9').val(val.ctsz9);
                $('#ctsz10').val(val.ctsz10);
                $('#ctsz11').val(val.ctsz11);
                $('#ctsz12').val(val.ctsz12);
                $('#ctsz13').val(val.ctsz13);
                $('#ctsz14').val(val.ctsz14);
                $('#ctsz15').val(val.ctsz15);
                $('#ctsz16').val(val.ctsz16);
                $('#ctsz17').val(val.ctsz17);
                $('#ctsz18').val(val.ctsz18);
                $('#ctsz19').val(val.ctsz19);
                $('#ctsz20').val(val.ctsz20);
                $('#ctsz21').val(val.ctsz21);
                $('#ctsz22').val(val.ctsz22);
                $('#ctsz23').val(val.ctsz23);
                $('#ctsz24').val(val.ctsz24);
                $('#ctsz25').val(val.ctsz25);
            }
        });
    });
    function postDataCTSZ() {
        var ctsz1 = $("#ctsz1").val();
        var ctsz2 = $("#ctsz2").val();
        var ctsz3 = $("#ctsz3").val();
        var ctsz4 = $("#ctsz4").val();
        var ctsz5 = $("#ctsz5").val();
        var ctsz6 = $("#ctsz6").val();
        var ctsz7 = $("#ctsz7").val();
        var ctsz8 = $("#ctsz8").val();
        var ctsz9 = $("#ctsz9").val();
        var ctsz10 = $("#ctsz10").val();
        var ctsz11 = $("#ctsz11").val();
        var ctsz12 = $("#ctsz12").val();
        var ctsz13 = $("#ctsz13").val();
        var ctsz14 = $("#ctsz14").val();
        var ctsz15 = $("#ctsz15").val();
        var ctsz16 = $("#ctsz16").val();
        var ctsz17 = $("#ctsz17").val();
        var ctsz18 = $("#ctsz18").val();
        var ctsz19 = $("#ctsz19").val();
        var ctsz20 = $("#ctsz20").val();
        var ctsz21 = $("#ctsz21").val();
        var ctsz22 = $("#ctsz22").val();
        var ctsz23 = $("#ctsz23").val();
        var ctsz24 = $("#ctsz24").val();
        var ctsz25 = $("#ctsz25").val();

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/AdvancedSetting/postDataCTSZ', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: {
                deviceID: va, fileName: va1,
                ctsz1: ctsz1, ctsz2: ctsz2, ctsz3: ctsz3, ctsz4: ctsz4, ctsz5: ctsz5,
                ctsz6: ctsz6, ctsz7: ctsz7, ctsz8: ctsz8, ctsz9: ctsz9, ctsz10: ctsz10,
                ctsz11: ctsz11, ctsz12: ctsz12, ctsz13: ctsz13, ctsz14: ctsz14, ctsz15: ctsz15,
                ctsz16: ctsz16, ctsz17: ctsz17, ctsz18: ctsz18, ctsz19: ctsz19, ctsz20: ctsz20,
                ctsz21: ctsz21, ctsz22: ctsz22, ctsz23: ctsz23, ctsz24: ctsz24, ctsz25: ctsz25
            },        //要传递的数据
            success: function (result) {
                if (result == "Success") {
                    alert("提交成功！");
                }
                else {
                    alert("提交失败！");
                }
            }
        });
    }
</script>

