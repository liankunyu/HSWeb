<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //初始化
            $.ajax({
                url: '/AdvancedSetting/SendDataBZYSD', //后台处理程序
                type: 'post',         //数据发送方式
                dataType: 'text',     //接受数据格式
                data: { deviceID: va, fileName: va1 },        //要传递的数据
                success: function (result) {

                    var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                    $('#bzysd1').val(val.bzysd1);
                    $('#bzysd2').val(val.bzysd2);
                    $('#bzysd3').val(val.bzysd3);
                    $('#bzysd4').val(val.bzysd4);
                    $('#bzysd5').val(val.bzysd5);
                    $('#bzysd6').val(val.bzysd6);
                    $('#bzysd7').val(val.bzysd7);
                    $('#bzysd8').val(val.bzysd8);
                    $('#bzysd9').val(val.bzysd9);
                    $('#bzysd10').val(val.bzysd10);
                    $('#bzysd11').val(val.bzysd11);
                    $('#bzysd12').val(val.bzysd12);
                    $('#bzysd13').val(val.bzysd13);
                    $('#bzysd14').val(val.bzysd14);
                    $('#bzysd15').val(val.bzysd15);
                    $('#bzysd16').val(val.bzysd16);
                    $('#bzysd17').val(val.bzysd17);
                    $('#bzysd18').val(val.bzysd18);
                    $('#bzysd19').val(val.bzysd19);
                    $('#bzysd20').val(val.bzysd20);
                }
            });
        });
        function postDataBZYSD() {
            var bzysd1 = $("#bzysd1").val();
            var bzysd2 = $("#bzysd2").val();
            var bzysd3 = $("#bzysd3").val();
            var bzysd4 = $("#bzysd4").val();
            var bzysd5 = $("#bzysd5").val();
            var bzysd6 = $("#bzysd6").val();
            var bzysd7 = $("#bzysd7").val();
            var bzysd8 = $("#bzysd8").val();
            var bzysd9 = $("#bzysd9").val();
            var bzysd10 = $("#bzysd10").val();
            var bzysd11 = $("#bzysd11").val();
            var bzysd12 = $("#bzysd12").val();
            var bzysd13 = $("#bzysd13").val();
            var bzysd14 = $("#bzysd14").val();
            var bzysd15 = $("#bzysd15").val();
            var bzysd16 = $("#bzysd16").val();
            var bzysd17 = $("#bzysd17").val();
            var bzysd18 = $("#bzysd18").val();
            var bzysd19 = $("#bzysd19").val();
            var bzysd20 = $("#bzysd20").val();

            var va = getCookie("deviceID");
            var va1 = getCookie("fileName");
            $.ajax({
                url: '/AdvancedSetting/postDataBZYSD', //后台处理程序
                type: 'post',         //数据发送方式
                dataType: 'text',     //接受数据格式
                data: {
                    deviceID: va, fileName: va1,
                    bzysd1: bzysd1, bzysd2: bzysd2, bzysd3: bzysd3, bzysd4: bzysd4, bzysd5: bzysd5,
                    bzysd6: bzysd6, bzysd7: bzysd7, bzysd8: bzysd8, bzysd9: bzysd9, bzysd10: bzysd10,
                    bzysd11: bzysd11, bzysd12: bzysd12, bzysd13: bzysd13, bzysd14: bzysd14, bzysd15: bzysd15,
                    bzysd16: bzysd16, bzysd17: bzysd17, bzysd18: bzysd18, bzysd19: bzysd19, bzysd20: bzysd20,
                },        //要传递的数据
                success: function (result) {
                    if (result == "Success") {
                        alert("提交成功！");
                    }
                    else {
                        alert("提交失败！");
                    }
                }
            });
        }
</script>

