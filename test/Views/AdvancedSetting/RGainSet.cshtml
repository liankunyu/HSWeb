<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //初始化
            $.ajax({
                url: '/AdvancedSetting/SendDataRZYSD', //后台处理程序
                type: 'post',         //数据发送方式
                dataType: 'text',     //接受数据格式
                data: { deviceID: va, fileName: va1 },        //要传递的数据
                success: function (result) {

                    var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                    $('#rzysd1').val(val.rzysd1);
                    $('#rzysd2').val(val.rzysd2);
                    $('#rzysd3').val(val.rzysd3);
                    $('#rzysd4').val(val.rzysd4);
                    $('#rzysd5').val(val.rzysd5);
                    $('#rzysd6').val(val.rzysd6);
                    $('#rzysd7').val(val.rzysd7);
                    $('#rzysd8').val(val.rzysd8);
                    $('#rzysd9').val(val.rzysd9);
                    $('#rzysd10').val(val.rzysd10);
                    $('#rzysd11').val(val.rzysd11);
                    $('#rzysd12').val(val.rzysd12);
                    $('#rzysd13').val(val.rzysd13);
                    $('#rzysd14').val(val.rzysd14);
                    $('#rzysd15').val(val.rzysd15);
                    $('#rzysd16').val(val.rzysd16);
                    $('#rzysd17').val(val.rzysd17);
                    $('#rzysd18').val(val.rzysd18);
                    $('#rzysd19').val(val.rzysd19);
                    $('#rzysd20').val(val.rzysd20);
                }
            });
        });
        function postDataRZYSD() {
            var rzysd1 = $("#rzysd1").val();
            var rzysd2 = $("#rzysd2").val();
            var rzysd3 = $("#rzysd3").val();
            var rzysd4 = $("#rzysd4").val();
            var rzysd5 = $("#rzysd5").val();
            var rzysd6 = $("#rzysd6").val();
            var rzysd7 = $("#rzysd7").val();
            var rzysd8 = $("#rzysd8").val();
            var rzysd9 = $("#rzysd9").val();
            var rzysd10 = $("#rzysd10").val();
            var rzysd11 = $("#rzysd11").val();
            var rzysd12 = $("#rzysd12").val();
            var rzysd13 = $("#rzysd13").val();
            var rzysd14 = $("#rzysd14").val();
            var rzysd15 = $("#rzysd15").val();
            var rzysd16 = $("#rzysd16").val();
            var rzysd17 = $("#rzysd17").val();
            var rzysd18 = $("#rzysd18").val();
            var rzysd19 = $("#rzysd19").val();
            var rzysd20 = $("#rzysd20").val();

            var va = getCookie("deviceID");
            var va1 = getCookie("fileName");
            $.ajax({
                url: '/AdvancedSetting/postDataRZYSD', //后台处理程序
                type: 'post',         //数据发送方式
                dataType: 'text',     //接受数据格式
                data: {
                    deviceID: va, fileName: va1,
                    rzysd1: rzysd1, rzysd2: rzysd2, rzysd3: rzysd3, rzysd4: rzysd4, rzysd5: rzysd5,
                    rzysd6: rzysd6, rzysd7: rzysd7, rzysd8: rzysd8, rzysd9: rzysd9, rzysd10: rzysd10,
                    rzysd11: rzysd11, rzysd12: rzysd12, rzysd13: rzysd13, rzysd14: rzysd14, rzysd15: rzysd15,
                    rzysd16: rzysd16, rzysd17: rzysd17, rzysd18: rzysd18, rzysd19: rzysd19, rzysd20: rzysd20,
                },        //要传递的数据
                success: function (result) {
                    if (result == "Success") {
                        alert("提交成功！");
                    }
                    else {
                        alert("提交失败！");
                    }
                }
            });
        }
</script>

