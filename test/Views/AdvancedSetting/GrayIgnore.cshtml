<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //传入初值
        $.ajax({
            url: '/AdvancedSetting/SendDataHDHL', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#hdhl1').val(val.hdhl1);
                $('#hdhl2').val(val.hdhl2);
                $('#hdhl3').val(val.hdhl3);
                $('#hdhl4').val(val.hdhl4);
                $('#hdhl5').val(val.hdhl5);
                $('#hdhl6').val(val.hdhl6);
                $('#hdhl7').val(val.hdhl7);
                $('#hdhl8').val(val.hdhl8);
                $('#hdhl9').val(val.hdhl9);
                $('#hdhl10').val(val.hdhl10);
                $('#hdhl11').val(val.hdhl11);
                $('#hdhl12').val(val.hdhl12);
                $('#hdhl13').val(val.hdhl13);
                $('#hdhl14').val(val.hdhl14);
                $('#hdhl15').val(val.hdhl15);
                $('#hdhl16').val(val.hdhl16);
                $('#hdhl17').val(val.hdhl17);
                $('#hdhl18').val(val.hdhl18);
                $('#hdhl19').val(val.hdhl19);
                $('#hdhl20').val(val.hdhl20);
            }
        });
    });
    function postDataHDHL() {
        var hdhl1 = $("#hdhl1").val();
        var hdhl2 = $("#hdhl2").val();
        var hdhl3 = $("#hdhl3").val();
        var hdhl4 = $("#hdhl4").val();
        var hdhl5 = $("#hdhl5").val();
        var hdhl6 = $("#hdhl6").val();
        var hdhl7 = $("#hdhl7").val();
        var hdhl8 = $("#hdhl8").val();
        var hdhl9 = $("#hdhl9").val();
        var hdhl10 = $("#hdhl10").val();
        var hdhl11 = $("#hdhl11").val();
        var hdhl12 = $("#hdhl12").val();
        var hdhl13 = $("#hdhl13").val();
        var hdhl14 = $("#hdhl14").val();
        var hdhl15 = $("#hdhl15").val();
        var hdhl16 = $("#hdhl16").val();
        var hdhl17 = $("#hdhl17").val();
        var hdhl18 = $("#hdhl18").val();
        var hdhl19 = $("#hdhl19").val();
        var hdhl20 = $("#hdhl20").val();

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/AdvancedSetting/postDataHDHL', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: {
                deviceID: va, fileName: va1,
                hdhl1: hdhl1, hdhl2: hdhl2, hdhl3: hdhl3, hdhl4: hdhl4, hdhl5: hdhl5,
                hdhl6: hdhl6, hdhl7: hdhl7, hdhl8: hdhl8, hdhl9: hdhl9, hdhl10: hdhl10,
                hdhl11: hdhl11, hdhl12: hdhl12, hdhl13: hdhl13, hdhl14: hdhl14, hdhl15: hdhl15,
                hdhl16: hdhl16, hdhl17: hdhl17, hdhl18: hdhl18, hdhl19: hdhl19, hdhl20: hdhl20,
            },        //要传递的数据
            success: function (result) {
                if (result == "Success") {
                    alert("提交成功！");
                }
                else {
                    alert("提交失败！");
                }
            }
        });
    }
</script>

