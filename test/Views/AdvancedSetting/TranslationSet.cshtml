<script type="text/javascript">
    //离开界面提示
    //window.onbeforeunload = function (e) {
    //    var e = window.event || e;
    //    e.returnValue = ("确定离开当前页面吗？");
    //}
    $(function () {
        //改颜色
        document.getElementById('Advanced_id').style.background = '#48484A';
        document.getElementById('Advanced_id').style.borderButton = '1px solid ';
        document.getElementById('Advanced_id').style.padding = '7px 12px 17px 12px ';
        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (0 <= parseInt(newValue) && parseInt(newValue) <= 245) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        //传入初值
        $.ajax({
            url: '/AdvancedSetting/SendDataPYSD', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#pysd1').val(val.pysd1);
                $('#pysd2').val(val.pysd2);
                $('#pysd3').val(val.pysd3);
                $('#pysd4').val(val.pysd4);
                $('#pysd5').val(val.pysd5);
                $('#pysd6').val(val.pysd6);
                $('#pysd7').val(val.pysd7);
                $('#pysd8').val(val.pysd8);
                $('#pysd9').val(val.pysd9);
                $('#pysd10').val(val.pysd10);
                $('#pysd11').val(val.pysd11);
                $('#pysd12').val(val.pysd12);
                $('#pysd13').val(val.pysd13);
                $('#pysd14').val(val.pysd14);
                $('#pysd15').val(val.pysd15);
                $('#pysd16').val(val.pysd16);
                $('#pysd17').val(val.pysd17);
                $('#pysd18').val(val.pysd18);
                $('#pysd19').val(val.pysd19);
                $('#pysd20').val(val.pysd20);
            }
        });
    });
    function postDataPYSD() {
        var pysd1 = $("#pysd1").val();
        var pysd2 = $("#pysd2").val();
        var pysd3 = $("#pysd3").val();
        var pysd4 = $("#pysd4").val();
        var pysd5 = $("#pysd5").val();
        var pysd6 = $("#pysd6").val();
        var pysd7 = $("#pysd7").val();
        var pysd8 = $("#pysd8").val();
        var pysd9 = $("#pysd9").val();
        var pysd10 = $("#pysd10").val();
        var pysd11 = $("#pysd11").val();
        var pysd12 = $("#pysd12").val();
        var pysd13 = $("#pysd13").val();
        var pysd14 = $("#pysd14").val();
        var pysd15 = $("#pysd15").val();
        var pysd16 = $("#pysd16").val();
        var pysd17 = $("#pysd17").val();
        var pysd18 = $("#pysd18").val();
        var pysd19 = $("#pysd19").val();
        var pysd20 = $("#pysd20").val();

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/AdvancedSetting/postDataPYSD', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: {
                deviceID: va, fileName: va1,
                pysd1: pysd1, pysd2: pysd2, pysd3: pysd3, pysd4: pysd4, pysd5: pysd5,
                pysd6: pysd6, pysd7: pysd7, pysd8: pysd8, pysd9: pysd9, pysd10: pysd10,
                pysd11: pysd11, pysd12: pysd12, pysd13: pysd13, pysd14: pysd14, pysd15: pysd15,
                pysd16: pysd16, pysd17: pysd17, pysd18: pysd18, pysd19: pysd19, pysd20: pysd20,
            },        //要传递的数据
            success: function (result) {
                if (result == "Success") {
                    alert("提交成功！");
                }
                else {
                    alert("提交失败！");
                }
            }
        });
    }
</script>

