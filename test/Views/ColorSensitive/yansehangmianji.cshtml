<script type="text/javascript">
    $(function () {

        document.getElementById('Color_id').style.background = '#48484A';
        document.getElementById('Color_id').style.borderButton = '1px solid ';
        document.getElementById('Color_id').style.padding = '7px 12px 17px 12px ';

        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (1 <= parseInt(newValue) && parseInt(newValue) <= 100) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/ColorSensitive/SendDataYSHMJ', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#hang1').val(val.hang1);
                $('#hang2').val(val.hang2);
                $('#hang3').val(val.hang3);
                $('#hang4').val(val.hang4);
                $('#hang5').val(val.hang5);
                $('#hang6').val(val.hang6);
                $('#hang7').val(val.hang7);
                $('#hang8').val(val.hang8);
                $('#hang9').val(val.hang9);
                $('#hang10').val(val.hang10);
                $('#hang11').val(val.hang11);
                $('#hang12').val(val.hang12);
                $('#hang13').val(val.hang13);
                $('#hang14').val(val.hang14);
                $('#hang15').val(val.hang15);
                $('#hang16').val(val.hang16);
                $('#hang17').val(val.hang17);
                $('#hang18').val(val.hang18);
                $('#hang19').val(val.hang19);
                $('#hang20').val(val.hang20);
            }
        });
    });
    function postDataYSHMJ() {
        var hang1 = $("#hang1").val();
        var hang2 = $("#hang2").val();
        var hang3 = $("#hang3").val();
        var hang4 = $("#hang4").val();
        var hang5 = $("#hang5").val();
        var hang6 = $("#hang6").val();
        var hang7 = $("#hang7").val();
        var hang8 = $("#hang8").val();
        var hang9 = $("#hang9").val();
        var hang10 = $("#hang10").val();
        var hang11 = $("#hang11").val();
        var hang12 = $("#hang12").val();
        var hang13 = $("#hang13").val();
        var hang14 = $("#hang14").val();
        var hang15 = $("#hang15").val();
        var hang16 = $("#hang16").val();
        var hang17 = $("#hang17").val();
        var hang18 = $("#hang18").val();
        var hang19 = $("#hang19").val();
        var hang20 = $("#hang20").val();

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/ColorSensitive/postDataYSHMJ', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: {
                deviceID: va, fileName: va1,
                hang1: hang1, hang2: hang2, hang3: hang3, hang4: hang4, hang5: hang5,
                hang6: hang6, hang7: hang7, hang8: hang8, hang9: hang9, hang10: hang10,
                hang11: hang11, hang12: hang12, hang13: hang13, hang14: hang14, hang15: hang15,
                hang16: hang16, hang17: hang17, hang18: hang18, hang19: hang19, hang20: hang20,
            },        //要传递的数据
            success: function (result) {
                if (result == "Success") {
                    alert("提交成功！");
                }
                else {
                    alert("提交失败！");
                }
            }
        });
    }

</script>
