<script type="text/javascript">
    $(function () {

        document.getElementById('Color_id').style.background = '#48484A';
        document.getElementById('Color_id').style.borderButton = '1px solid ';
        document.getElementById('Color_id').style.padding = '7px 12px 17px 12px ';

        var oldValue = "0";
        $("input").focus(function () {
            oldValue = $(this).val();
        });

        $("input").blur(function () {
            var newValue = $(this).val();
            if (!isNaN(newValue)) {
                if (1 <= parseInt(newValue) && parseInt(newValue) <= 100) {
                    $(this).val() = parseInt(newValue);
                    return;
                }
                else {
                    alert("请输入0-255之间的整数");
                    $(this).val(oldValue);
                }
            }
            else {
                alert("请输入整数");
                $(this).val(oldValue);
            }
        });

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/ColorSensitive/SendDataBJYZ', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: { deviceID: va, fileName: va1 },        //要传递的数据
            success: function (result) {

                var val = eval('(' + result + ')'); //把传过来的json字符串变换成json对象

                $('#bei1').val(val.bei1);
                $('#bei2').val(val.bei2);
                $('#bei3').val(val.bei3);
                $('#bei4').val(val.bei4);
                $('#bei5').val(val.bei5);
                $('#bei6').val(val.bei6);
                $('#bei7').val(val.bei7);
                $('#bei8').val(val.bei8);
                $('#bei9').val(val.bei9);
                $('#bei10').val(val.bei10);
                $('#bei11').val(val.bei11);
                $('#bei12').val(val.bei12);
                $('#bei13').val(val.bei13);
                $('#bei14').val(val.bei14);
                $('#bei15').val(val.bei15);
                $('#bei16').val(val.bei16);
                $('#bei17').val(val.bei17);
                $('#bei18').val(val.bei18);
                $('#bei19').val(val.bei19);
                $('#bei20').val(val.bei20);
            }
        });
    });
    function postDataBJYZ() {
        var bei1 = $("#bei1").val();
        var bei2 = $("#bei2").val();
        var bei3 = $("#bei3").val();
        var bei4 = $("#bei4").val();
        var bei5 = $("#bei5").val();
        var bei6 = $("#bei6").val();
        var bei7 = $("#bei7").val();
        var bei8 = $("#bei8").val();
        var bei9 = $("#bei9").val();
        var bei10 = $("#bei10").val();
        var bei11 = $("#bei11").val();
        var bei12 = $("#bei12").val();
        var bei13 = $("#bei13").val();
        var bei14 = $("#bei14").val();
        var bei15 = $("#bei15").val();
        var bei16 = $("#bei16").val();
        var bei17 = $("#bei17").val();
        var bei18 = $("#bei18").val();
        var bei19 = $("#bei19").val();
        var bei20 = $("#bei20").val();

        var va = getCookie("deviceID");
        var va1 = getCookie("fileName");
        $.ajax({
            url: '/ColorSensitive/postDataBJYZ', //后台处理程序
            type: 'post',         //数据发送方式
            dataType: 'text',     //接受数据格式
            data: {
                deviceID: va, fileName: va1,
                bei1: bei1, bei2: bei2, bei3: bei3, bei4: bei4, bei5: bei5,
                bei6: bei6, bei7: bei7, bei8: bei8, bei9: bei9, bei10: bei10,
                bei11: bei11, bei12: bei12, bei13: bei13, bei14: bei14, bei15: bei15,
                bei16: bei16, bei17: bei17, bei18: bei18, bei19: bei19, bei20: bei20,
            },        //要传递的数据
            success: function (result) {
                if (result == "Success") {
                    alert("提交成功！");
                }
                else {
                    alert("提交失败！");
                }
            }
        });
    }

</script>
